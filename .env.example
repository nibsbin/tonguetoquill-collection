# Development Environment Variables Template
# Copy this file to .env and fill in the values

# Application Settings
NODE_ENV=development

# Mock Provider Flags (Phases 1-9)
USE_AUTH_MOCKS=true
USE_DB_MOCKS=true

# Mock JWT Secret (Development only - DO NOT use in production)
MOCK_JWT_SECRET=dev-secret-key-change-in-production

# OAuth Configuration
AUTH_REDIRECT_URI=http://localhost:5173/api/auth/callback

# Supabase Configuration (Phase 10+)
# Uncomment and fill in when integrating real Supabase
# USE_AUTH_MOCKS=false
# USE_DB_MOCKS=false
# SUPABASE_URL=https://your-project.supabase.co
# SUPABASE_ANON_KEY=your-anon-key
# SUPABASE_SERVICE_ROLE_KEY=your-service-key
# SUPABASE_JWT_SECRET=your-jwt-secret
